SELECT  MAX(GPA),BRANCH
FROM Student
GROUP BY BRANCH;
+----------+--------+
| MAX(GPA) | BRANCH |
+----------+--------+
|    73.33 | CS     |
|    90.00 | MCA    |
+----------+--------+

SELECT NAME
FROM Student
WHERE NAME like "S%";
+-------+
| NAME  |
+-------+
| Sunil |
+-------+

select name from Student
where NAME like "%ar";

+-------+
| name  |
+-------+
| Kumar |
+-------+

SELECT br.USN
FROM BORROW br,BOOK bk
   WHERE br.bookid AND br.USN=S.USN
    GROUP BY USN
    HAVING COUNT(DISTINCT authorid)>1;


select USN FROM BORROW 
GROUP BY USN;
+------------+
| USN        |
+------------+
| 4VP18CS058 |
| 4VP19ME021 |
| 4VP20MB010 |
| 4VP21MC002 |
+------------+


select USN FROM BORROW,AUTHOR
GROUP BY USN
HAVING COUNT(authorid)>1;
+------------+
| USN        |
+------------+
| 4VP19ME021 |
| 4VP18CS058 |
| 4VP20MB010 |
| 4VP21MC002 |
+------------+

SELECT USN,publisher 
from BORROW,BOOK
WHERE BORROW.bookid=BOOK.bookid
AND
s.USN=BORROW.USN
GROUP BY USN
HAVING COUNT(DISTINCT publisher)=1);


delete  from Student
where usn="4VP21MC009";
