create table Student
(USN varchar(20) primary key,
NAME varchar(20) not null,
DOB date,
BRANCH char(10),
MARK1 float(5,2),
MARK2 float(5,2),
MARK3 float(5,2),
TOTAL float(5,2),
GPA float(5,2));

insert into Student(USN,NAME,DOB,BRANCH,MARK1,MARK2,MARK3) values('4VP18CS066','Kumar','2000-11-17','CS',50,60,80);

mysql> select * from Student;
